    "chai": "^6.2.1",
    "mocha": "^11.7.1",
    "publint": "^0.3.9",
    "sinon": "^21.0.0",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
