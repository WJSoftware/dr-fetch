  "type": "module",
  "scripts": {
    "test": "ts-mocha -n loader=ts-node/esm -p ./tsconfig.json ./tests/**/*.test.ts",
    "build": "tsc && publint",
    "prebuild": "npm run test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fetch"
  ],
  "author": {
    "email": "webJose@gmail.com",
